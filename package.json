{
  "name": "auto-import",
  "version": "0.1.0",
  "description": "A tool for automatically update and import NPM module",
  "main": "index.js",
  "scripts": {
    "test": "npm run cov",
    "dev": "tsc --watch",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifee-team/auto-import.git"
  },
  "keywords": [
    "auto",
    "import",
    "module"
  ],
  "author": "ifee-team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifee-team/auto-import/issues"
  },
  "homepage": "https://github.com/ifee-team/auto-import#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/node": "^10.17.60",
    "@types/node-fetch": "^2.5.12",
    "@types/request": "^2.48.7",
    "@types/semver": "^7.3.8",
    "lint-staged": "^9.5.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "fs-extra": "^10.0.0",
    "jest": "^27.1.1",
    "npminstall": "^4.11.0",
    "request": "^2.88.2",
    "semver": "^7.3.5"
  },
  "engines": {
    "node": ">=10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
